{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
	"exclude": ["src/**/__tests__/*", "node_modules"],
	"compilerOptions": {
		"emitDeclarationOnly": true,
		"declaration": true,
		"baseUrl": ".",
		"declarationDir": "dist/types",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
